{
  "name": "@hendacorp/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "volta": {
    "node": "20",
    "pnpm": "9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akhenda/hendacorp"
  },
  "nx": {
    "includedScripts": []
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "gc": "git add . && cz",
    "gc-ai": "git add . && npx czg ai -N=5",
    "prepare": "husky install",
    "help": "nx help",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "nx": "pnpm exec nx",
    "nx:update": "nx migrate latest",
    "nx:test": "pnpm exec nx test",
    "nx:reset": "pnpm exec nx reset"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.0",
    "@commitlint/config-conventional": "17.8.0",
    "@commitlint/config-nx-scopes": "17.6.4",
    "@hendacorp/eslint-plugin": "workspace:*",
    "@nx/eslint-plugin": "19.5.7",
    "@nx/jest": "19.5.7",
    "@nx/js": "19.5.7",
    "@nx/workspace": "19.5.7",
    "@swc/cli": "0.3.14",
    "@swc/core": "1.5.7",
    "@swc/jest": "0.2.36",
    "@theunderscorer/nx-semantic-release": "2.6.2",
    "@types/jest": "29.5.6",
    "@types/node": "18.19.9",
    "cz-git": "1.7.1",
    "eslint": "8.57.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lint-staged": "15.0.2",
    "nx": "19.5.7",
    "prettier": "3.0.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.5.4",
    "verdaccio": "5.27.0",
    "@nx/eslint": "19.5.7"
  },
  "dependencies": {
    "@swc/helpers": "0.5.12",
    "tslib": "2.6.2"
  }
}
