{
  "name": "@hendacorp/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@9.8.0",
  "volta": {
    "node": "20.17.0",
    "pnpm": "9.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akhenda/hendacorp"
  },
  "nx": {
    "includedScripts": []
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "gc": "git add . && cz",
    "gc-ai": "git add . && npx czg ai -N=5",
    "prepare": "husky",
    "help": "nx help",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "nx": "pnpm exec nx",
    "nx:update": "nx migrate latest",
    "nx:test": "pnpm exec nx test",
    "nx:reset": "pnpm exec nx reset",
    "deps:update:": "pnpm exec nx run-many --target=update-deps --all",
    "build": "pnpm exec nx run-many --target=build --all",
    "test": "pnpm exec nx run-many --target=test --all",
    "lint": "pnpm exec nx run-many --target=lint --all",
    "lint:fix": "pnpm exec nx run-many --target=lint:fix --all"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/config-nx-scopes": "19.3.1",
    "@hendacorp/eslint-plugin": "workspace:*",
    "@nx/eslint-plugin": "19.6.2",
    "@nx/jest": "19.6.2",
    "@nx/js": "19.6.2",
    "@nx/workspace": "19.6.2",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.18",
    "@swc/jest": "0.2.36",
    "@theunderscorer/nx-semantic-release": "2.12.0",
    "@types/jest": "29.5.12",
    "@types/node": "22.5.0",
    "cz-git": "1.9.4",
    "eslint": "8.57.0",
    "husky": "9.1.5",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lint-staged": "15.2.9",
    "nx": "19.6.2",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "verdaccio": "5.32.1",
    "@nx/eslint": "19.6.2"
  },
  "dependencies": {
    "@swc/helpers": "0.5.12",
    "tslib": "2.7.0"
  }
}
